{
  "version": 2,
  "builds": [
    {
      "src": "apps/coinbridge-api/src/main.ts",
      "use": "@vercel/node",
      "scripts": {
        "build": "npm install --legacy-peer-deps && npm npx run coinbridge-api:build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/coinbridge-api/src/main.ts"
    }
  ]
}